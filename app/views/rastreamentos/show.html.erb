<div class="page-header">
  <%= link_to rastreamentos_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_rastreamento_path(@rastreamento), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show rastreamento</h1>
</div>

<dl class="dl-horizontal">
  <dt>Time:</dt>
  <dd><%= @rastreamento.time %></dd>

  <dt>Pessoa:</dt>
  <dd><%= @rastreamento.pessoa %></dd>

  <dt>Issue:</dt>
  <dd><%= @rastreamento.issue %></dd>

  <dt>Status:</dt>
  <dd><%= @rastreamento.status %></dd>

</dl>
